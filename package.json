  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.20.7",
    "material-ui": "^0.20.2",
    "morgan": "^1.9.1",
    "pg": "^7.17.0",
    "rapidapi-connect": "^0.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "thunk": "^0.0.1",
    "twilio": "^3.39.1"
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "serve": "react-scripts build && node server",
    "hack": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "yarn build && yarn migrate && yarn seed",
    "migrate": "knex migrate:latest --knexfile ./db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./db/knexfile.js",
    "seed": "knex seed:run --knexfile ./db/knexfile.js",
    "test:e2e:gui": "cypress open",
    "test:e2e:cli": "cypress run",
    "cypress:install": "cypress install",
    "test:wait": "wait-on http://localhost:4000 && yarn test:e2e:cli"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "cypress": "^3.8.1",
    "nodemon": "^2.0.2",
    "redux-actions-type": "^1.0.0",
    "typescript": "^3.7.4",
    "wait-on": "^3.3.0"
  }
}
