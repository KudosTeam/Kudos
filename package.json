{
  "name": "food-master",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "@material-ui/core": "^4.11.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "material-ui": "^0.20.2",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "rapidapi-connect": "^0.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "thunk": "^0.0.1",
    "twilio": "^3.55.1"
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "serve": "react-scripts build && node server",
    "hack": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "yarn build && yarn migrate && yarn seed",
    "migrate": "knex migrate:latest --knexfile ./db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./db/knexfile.js",
    "seed": "knex seed:run --knexfile ./db/knexfile.js",
    "test:e2e:gui": "cypress open",
    "test:e2e:cli": "cypress run",
    "cypress:install": "cypress install",
    "test:wait": "wait-on http://localhost:4000 && yarn test:e2e:cli"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "cypress": "^6.2.1",
    "nodemon": "^2.0.7",
    "redux-actions-type": "^1.0.0",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  }
}
